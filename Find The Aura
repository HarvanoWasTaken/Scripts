for i = 1, #workspace.Morphs:GetChildren(), 1 do 

if workspace.Morphs:GetChildren()[i].ClassName ~= "Script" then
local AuraName = workspace.Morphs:GetChildren()[i].Name

While Wait(0) do

if workspace.Morphs:GetChildren()[i].Name == AuraName then
Break
end

workspace.Morphs:GetChildren()[i].CFrame = workspace.nintenblock12.HumanoidRootPart.CFrame  

end

end 
end
